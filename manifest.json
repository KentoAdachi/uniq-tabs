{
    "manifest_version": 3,
    "name": "uniq-tabs",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs", "scripting"],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  }
  }
  